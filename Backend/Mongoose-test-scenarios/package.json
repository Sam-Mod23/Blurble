  "dependencies": {
    "migrate-mongoose": "^4.0.0",
    "mongodb": "^3.6.6",
    "mongoose": "^5.12.4",
    "mongoose-auto-increment": "^5.0.1",
    "node": "^15.14.0"
  }
}
